      <script>
                  const mainJson = `{
                                        "numofresults": 2,
                                        "data": [{
                                            "name" : "my name",
                                            "description" : "my des" 
                                        },{
                                            "name" : "my name2",
                                            "description" : "my des2"}
                                            ,{
                                            "name" : "my name2",
                                            "description" : "my des2"}
                                            ,{
                                            "name" : "my name2",
                                            "description" : "my des2"}
                                            
                                            
                            
                                        ]
                                    }`;


                    // Assuming you have a variable named jsonString containing your JSON data
                    const nestedJSON = JSON.parse(mainJson);
                    try{
                            console.log("parsed first");
                            console.log(nestedJSON.numofresults);
                            
                            const parsedData = nestedJSON.data;
                            for (let i = 0; i < parsedData.length; i++) {
                                const parsedNestedData = parsedData[i];
                               
                                // Create a new card element
                                const card = document.createElement('div');
                                card.classList.add('card', 'mb-6','mt-4');
                                card.style.borderColor = '#1A465A';

                                // Create card body
                                const cardBody = document.createElement('div');
                                cardBody.classList.add('card-body');

                                // Create card title and text
                                const cardTitle = document.createElement('h5');
                                cardTitle.classList.add('card-title', 'bluetext');
                                cardTitle.textContent = parsedNestedData.name; // Assuming name property

                                const cardText = document.createElement('p');
                                cardText.classList.add('card-text', 'bluetext');
                                cardText.textContent = parsedNestedData.description; // Assuming description property

                                // Append elements to create card structure
                                cardBody.appendChild(cardTitle);
                                cardBody.appendChild(cardText);
                                card.appendChild(cardBody);

                                // Append card to the results container
                                document.getElementById('search-results').appendChild(card);
                            
                            }
                        }
                        catch (error) {
                            console.error("Error parsing JSON data:", error);
                            // Handle JSON parsing error (e.g., display an error message to the user)
                        }
                
                </script>